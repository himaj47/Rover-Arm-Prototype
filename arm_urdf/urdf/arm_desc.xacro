<?xml version="1.0" ?>
<robot name="Part1" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:property name="PI" value="3.14159" />
    <xacro:property name="effort" value="30.0" />
    <xacro:property name="velocity" value="10.0" />
    <xacro:property name="damping" value="100" />
    <xacro:property name="friction" value="100" />

    <xacro:include filename="common.xacro"/>



    <link name="base_footprint">
        <xacro:inertial_box mass="10000" x="0.5" y="0.5" z="0.5">
            <origin xyz="0 0 0.075" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>


    <link name="base_link">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Base_Plate_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 0" rpy="${PI/2} 0 0" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Base_Plate_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 0" rpy="${PI/2} 0 0" />
        </collision>

        <xacro:inertial_box mass="100" x="0.5" y="0.5" z="0.5">
            <origin xyz="0 0 0.075" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <joint name="fixed_base_joint" type="fixed">
        <parent link="base_footprint" />
        <child link="base_link" />
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>
    
    <link name="revolute_base_link">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link1_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 -0.508" rpy="${PI/2} 0 0" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link1_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 -0.508" rpy="${PI/2} 0 0" />
        </collision>
    </link>
    
    <joint name="fixed_base_rev_base_joint" type="continuous">
        <parent link="base_link" />
        <child link="revolute_base_link" />
        <origin xyz="0 0 0.508" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="0" upper="${2*pi}" velocity="100" effort="100"/>
        <dynamics damping="${damping}" friction="${friction}"/>
    </joint>
    
    <link name="link2">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link2_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0.098 0 -0.805" rpy="${PI/2} 0 0" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link2_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0.098 0 -0.805" rpy="${PI/2} 0 0" />
        </collision>
    </link>
    
    <joint name="rev_base_link2_joint" type="continuous">
        <parent link="revolute_base_link" />
        <child link="link2" />
        <origin xyz="-0.100 -0.01 0.300" rpy="0 0 0.08" />  
        <axis xyz="0 1 0" />
        <limit lower="0" upper="${2*pi}" velocity="100" effort="100"/>
        <dynamics damping="${damping}" friction="${friction}"/>
    </joint>
    
    <link name="link3">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link3_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0.110 0 -3.15" rpy="${PI/2} 0 -0.02" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link3_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
        </collision>
    </link>
    
    <joint name="link2_link3_joint" type="continuous">
        <parent link="link2" />
        <child link="link3" />
        <origin xyz="-0.01 0 2.345" rpy="0 0 -0.05" />  
        <axis xyz="0 1 0" />
        <limit lower="0" upper="${2*pi}" velocity="100" effort="100"/>
        <dynamics damping="${damping}" friction="${friction}"/>
    </joint>
    
    <link name="link4">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link4_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0.098 0 -5.32" rpy="${PI/2} 0 0" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link4_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
        </collision>
    </link>
    
    <joint name="link3_link4_joint" type="continuous">
        <parent link="link3" />
        <child link="link4" />
        <origin xyz="0 -0.0055 2.168" rpy="0 0 0.05" />  
        <axis xyz="0 1 0" />
        <limit lower="0" upper="${2*pi}" velocity="100" effort="100"/>
        <dynamics damping="${damping}" friction="${friction}"/>
    </joint>
    
    <link name="link5">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link5_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0.098 0 -7.574" rpy="${PI/2} 0 0" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link5_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
        </collision>
    </link>
    
    <joint name="link4_link5_joint" type="continuous">
        <parent link="link4" />
        <child link="link5" />
        <origin xyz="0 0 2.253" rpy="0 0 0" />  
        <axis xyz="0 1 0" />
        <limit lower="0" upper="${2*pi}" velocity="100" effort="100"/>
        <dynamics damping="${damping}" friction="${friction}"/>
    </joint>
    
    <link name="gripper">
        <xacro:default_inertial mass="1.0" />
        <visual>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link6_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0.09 0.022 -8.285" rpy="${PI/2} 0 0" />
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find arm_urdf)/meshes/Link6_.stl" scale="0.01 0.01 0.01" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
        </collision>
    </link>
    
    <joint name="link5_gripper_joint" type="continuous">
        <parent link="link5" />
        <child link="gripper" />
        <origin xyz="0.01 -0.033 0.72" rpy="0 0 0" />  
        <axis xyz="0 0 1" />
        <limit lower="0" upper="${2*pi}" velocity="100" effort="100"/>
        <dynamics damping="${damping}" friction="${friction}"/>
    </joint>
    
    




    

    <joint name="gripper_camera_joint" type="fixed">
        <origin xyz="-0.3 0 1.5" rpy="0 ${PI/-2} 0"/>
        <parent link="gripper"/>
        <child link="camera"/>        
    </joint>


    <link name="camera">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.06 0.15 0.15" />
            </geometry>
            <material name="green" />
        </visual>

        <visual>
            <origin xyz="0.03 0 0" rpy="0 ${pi/2} 0"/>
            <geometry>
                <cylinder length="0.06" radius="0.04" />
            </geometry>
            <material name="blue" />

        </visual>
        <collision>
            <origin xyz="0.0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.12 0.15 0.15" />
            </geometry>
        </collision>

        <xacro:inertial_box mass="0.1" x="0.12" y="0.15" z="0.15">
            <origin xyz="0.0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>


    <joint name="camera_optical_joint" type="fixed">
        <origin xyz="0.13 0 0" rpy="0 0 0" />
        <parent link="camera" />
        <child link="camera_link_optical" />
    </joint>

    <link name="camera_link_optical"/>


    <xacro:include filename="gazebo.xacro"/>
    


</robot>
